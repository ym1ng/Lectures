apply plugin: 'com.android.application'

android {
    compileSdkVersion 25
    buildToolsVersion "25.0.2"
    defaultConfig {
        applicationId "com.example.administrator.lecturesmanagerdemo"
        minSdkVersion 16
        targetSdkVersion 25
        versionCode 1
        versionName "1.0"
        testInstrumentationRunner "android.support.Test.runner.AndroidJUnitRunner"
    }
    buildTypes {
        release {
            minifyEnabled false
            proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.pro'
        }
    }
}

dependencies {
    compile fileTree(include: ['*.jar'], dir: 'libs')
    compile 'com.android.support:appcompat-v7:25.3.1'
    compile 'com.android.support:cardview-v7:25.3.1'
    testCompile 'junit:junit:4.12'
    compile 'com.youth.xframe:xframe:1.0.2'
    compile 'com.squareup.okhttp3:okhttp:3.3.1'
    compile 'com.jakewharton:butterknife:6.1.0'
    //retrofit2
    compile 'com.squareup.retrofit2:retrofit:2.1.0'
    //retrofit
    compile 'com.google.code.gson:gson:2.6.2'
    //Gson 库
    //下面两个是RxJava 和RxAndroid
    compile 'io.reactivex:rxjava:1.1.0'
    compile 'io.reactivex:rxandroid:1.1.0'
    compile 'com.squareup.retrofit2:converter-gson:2.1.0'
    //转换器，请求结果转换成Model
    compile 'com.squareup.retrofit2:adapter-rxjava:2.1.0'
    //配合Rxjava 使用
    compile 'com.github.anzewei:parallaxbacklayout:0.1'
    compile 'com.jcodecraeer:xrecyclerview:1.3.2'
    compile 'com.github.bumptech.glide:glide:3.7.0'
    compile 'com.github.codbking:CalendarExaple:v1.0.0'
    compile 'com.squareup.picasso:picasso:2.5.2'
    compile 'org.greenrobot:eventbus:3.0.0'
    compile 'com.leon:lsettingviewlibrary:1.0'
    compile 'com.ycl.tabview.library:tabviewlibrary:1.0'
    compile 'com.zhy:base-rvadapter:3.0.3'
    compile 'com.zhy:base-adapter:3.0.3'
    compile 'com.youth.banner:banner:1.4.9'
    //最新版本
    compile 'com.zxy.android:recovery:0.0.8'
    compile 'com.github.dongjunkun:DropDownMenu:1.0.3'
    compile 'com.github.jdsjlzx:LRecyclerView:1.2.9'
    compile 'com.huxq17.xrefreshview:xrefreshview:3.6.6'
    compile 'com.github.JayFang1993:DropDownMenu:v0.9'
    compile 'me.leefeng:lfrecyclerview:1.0.1'
    compile 'fm.jiecao:jiecaovideoplayer:5.5.4'
    compile 'com.wang.avi:library:1.0.0'
    compile 'cn.qqtheme.framework:WheelPicker:1.1.2'
    compile 'cn.qqtheme.framework:FilePicker:1.1.2'
    compile 'cn.yipianfengye.android:zxing-library:2.1'
    //compile 'com.zaaach:citypicker:1.1'

    compile files('libs/Android_Map3D_SDK_V5.1.0_20170518.jar')
    compile files('libs/AMap_Location_V3.4.0_20170427.jar')
}
